/ JQuery-Template
%script{:id => "youtube-tmpl", :type => "text/x-jquery-tmpl"}
  %li
    .video_img
      %a{:href=> "${link}", :class => "fancybox-media"}
        %img{:src => "${img_url}"}  
    .video_name
      %a{:href=> "${link}"}
        ${name}
    .updated ${updated}
    .video_desc ${desc}

